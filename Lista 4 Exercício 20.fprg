    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="20.&#9;Exibir a tabuada da multiplica&#231;&#227;o de um n&#250;mero inteiro solicitado pelo usu&#225;rio. Por exemplo, para o n&#250;mero 5, temos: 5 x 1  = 5     5 x 2  = 10   ...  5 x 10 = 50"/>
            <declare name="n, c" type="Integer" array="False" size=""/>
            <input variable="n"/>
            <for variable="c" start="1" end="10" direction="inc" step="1">
                <output expression="n &amp; &quot; x &quot; &amp; c &amp; &quot; = &quot; &amp; (n*c)" newline="True"/>
            </for>
        </body>
    </function>
