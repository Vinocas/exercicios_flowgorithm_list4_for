    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="8.&#9;Calcular e exibir a soma dos n&#250;meros pares menores que 100, inclusive."/>
            <declare name="c, soma" type="Integer" array="False" size=""/>
            <assign variable="soma" expression="0"/>
            <for variable="c" start="0" end="100" direction="inc" step="1">
                <if expression="c mod 2 = 0">
                    <then>
                        <assign variable="soma" expression="soma + c"/>
                    </then>
                    <else/>
                </if>
            </for>
            <output expression="soma" newline="True"/>
        </body>
    </function>
