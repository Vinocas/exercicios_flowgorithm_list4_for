    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="12.&#9;Exibir os n primeiros termos da sequ&#234;ncia 1, 1, 2, 3, 5, 8, 13, 21, 34, &#8230;, onde n &#233; um n&#250;mero inteiro digitado pelo usu&#225;rio. "/>
            <declare name="c, seq1, seq2, n, t" type="Integer" array="False" size=""/>
            <input variable="n"/>
            <assign variable="seq1" expression="0"/>
            <assign variable="seq2" expression="1"/>
            <for variable="c" start="1" end="n" direction="inc" step="1">
                <output expression="seq2" newline="True"/>
                <assign variable="t" expression="seq2"/>
                <assign variable="seq2" expression="seq1 + seq2"/>
                <assign variable="seq1" expression="t"/>
            </for>
        </body>
    </function>
