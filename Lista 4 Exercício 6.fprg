    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="6.&#9;Com base no exerc&#237;cio anterior, calcular e exibir o fatorial de n, onde n &#233; um n&#250;mero maior ou igual a zero,  digitado pelo usu&#225;rio. &#13;&#10;Observa&#231;&#227;o: o fatorial de zero &#233; igual a 1.&#13;&#10;"/>
            <declare name="serie, vez, n" type="Integer" array="False" size=""/>
            <assign variable="serie" expression="1"/>
            <assign variable="vez" expression="1"/>
            <input variable="n"/>
            <if expression="n = 0">
                <then>
                    <output expression="1" newline="True"/>
                </then>
                <else>
                    <for variable="vez" start="1" end="n" direction="inc" step="1">
                        <assign variable="serie" expression="serie * vez"/>
                        <output expression="serie" newline="True"/>
                    </for>
                </else>
            </if>
        </body>
    </function>
