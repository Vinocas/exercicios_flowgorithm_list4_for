    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <comment text="16.&#9;Somat&#243;rio: Construa os algoritmos que calculem os seguintes somat&#243;rios:&#13;&#10;a.&#9;  &#13;&#10;b.&#9; "/>
            <declare name="c, soma, expoente" type="Integer" array="False" size=""/>
            <assign variable="soma" expression="0"/>
            <for variable="c" start="1" end="5" direction="inc" step="1">
                <assign variable="soma" expression="soma + c"/>
            </for>
            <assign variable="expoente" expression="soma * soma"/>
            <output expression="&quot;K &#233; igual a = &quot; &amp; expoente" newline="True"/>
        </body>
    </function>
