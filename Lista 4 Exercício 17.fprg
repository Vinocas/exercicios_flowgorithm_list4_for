            <for variable="lin" start="1" end="3" direction="inc" step="1">
                <for variable="col" start="1" end="5" direction="inc" step="1">
                    <output expression="lin &amp; col &amp; &quot; &quot;" newline="False"/>
                </for>
                <output expression="tochar(13)" newline="False"/>
            </for>
